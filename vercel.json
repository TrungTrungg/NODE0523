{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
  { "src": "/static/(.*)", "dest": "/static/$1" },
  { "src": "/api/(.*)", "dest": "index.js" },
  { "src": "/(.*)", "dest": "index.js" }
],
  "env": {
    "SECRET_KEY":"secret",
    "DB_USER":"xuantrung",
    "DB_PASSWORD":"xuantrung",
    "TOKEN_EXPIRES":"2400s",
    "CRYPTO_KEY":"secret"
  },
  "public": true
}
